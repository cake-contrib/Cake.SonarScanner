    <members>
        <member name="T:Cake.SonarScanner.SonarScannerAliases">
            <summary>
            Contains functionality for using the Sonar Scanner tool
            </summary>
            <code>
            <![CDATA[
            #addin "nuget:?package=Cake.SonarScanner"
            ]]>
            </code>
        </member>
        <member name="M:Cake.SonarScanner.SonarScannerAliases.SonarScanner(Cake.Core.ICakeContext)">
            <summary>
            Runs SonarScanner with the default settings
            </summary>
            <example>
            <para>Use the #addin preprocessor directive</para>
            <code>
            <![CDATA[
            #addin "nuget:?package=Cake.SonarScanner"
            ]]>
            </code>
            <para>Cake task:</para>
            <code>
            <![CDATA[
            Task("SomeTask").Does(() =>  SonarScanner());
            ]]>
            </code>
            </example>
            <param name="context">The context.</param>
        </member>
        <member name="M:Cake.SonarScanner.SonarScannerAliases.SonarScanner(Cake.Core.ICakeContext,Cake.SonarScanner.SonarScannerSettings)">
            <summary>
            Runs SonarScanner with the specified settings
            </summary>
            <example>
            <para>Use the #addin preprocessor directive</para>
            <code>
            <![CDATA[
            #addin "nuget:?package=Cake.SonarScanner"
            ]]>
            </code>
            <para>Cake task:</para>
            <code>
            <![CDATA[
            Task("SomeTask").Does(() =>  SonarScanner(new SonarScannerSettings
            {
            });
            ]]>
            </code>
            </example>
            <param name="context">The context.</param>
            <param name="settings">The SonarScannerSettings</param>
        </member>
        <member name="T:Cake.SonarScanner.SonarScannerSettings">
            <summary>
            Settings for the aliases
            </summary>
        </member>
